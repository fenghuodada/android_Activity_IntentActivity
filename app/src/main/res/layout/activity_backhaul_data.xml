<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".BackhaulData">
    <TextView
        android:id="@+id/BackhaulData_text_1"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:textSize="20dp"
        android:gravity="center"
        android:layout_alignParentTop="true"
        android:layout_marginTop="200dp"
        android:text="请点击按钮"/>
    <Button
        android:id="@+id/BackhaulData_button_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/BackhaulData_text_1"
        android:textSize="20dp"
        android:text="计算和" />
    <Button
        android:id="@+id/BackhaulData_button_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/BackhaulData_button_1"
        android:layout_marginTop="10dp"
        android:textSize="20dp"
        android:text="返回和" />


</RelativeLayout>